{
    "datasets": {
      "train": {
        "dir": "../input/processed/train/*[jpg,tif]",
        "csv": "../input/processed/train.csv",
        "num_folds": 10,
        "val_fold": 1
      },
      "test": {
        "dir": "../input/processed/test/*[jpg,tif]",
        "csv": "../input/processed/test.csv"
      },
      "real": {
        "dir": "../input/processed/real/*[jpg,tif]",
        "csv": "../input/processed/real.csv"
      },
      "label_encoder": "../input/label_encoder.npy"
    },
    "model": {
      "id": "R18",
      "type": "resnet18",
      "dir": "../models/",
      "size": [300, 300],
      "n_classes": 7
    },
    "DataLoader": {
      "batch_size": 12,
      "num_workers": 11
    },
    "optimizer": {
      "learning_rate": 0.0001,
      "betas": [0.8, 0.99],
      "epsilon": 0.001,
      "weight_decay": 0.0001
    },
    "scheduler": {
      "patience": 30,
      "mode": "max"
    },
    "training": {
      "epoch": 500
    }
  }